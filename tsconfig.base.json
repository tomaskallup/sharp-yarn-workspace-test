{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2017",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "useUnknownInCatchVariables": false,
    "sourceMap": true,
    "stripInternal": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "incremental": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "lib": [
      "es2020"
    ]
  }
}
